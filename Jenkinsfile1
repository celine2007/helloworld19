 pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'Hello build'
            sleep 10
        }
     }   
     stages {
        stage('test') {
            steps {
                echo 'Hello test'
            sleep 5
        }
    }

  

    stages {
        stage('deploy') {
            steps {
                echo 'Hello deploy'
            pwd
        }
    }

   stages {
        stage('push') {
            steps {
                echo 'Hello push'
            sh 'docker ps'
        }
    }
}
